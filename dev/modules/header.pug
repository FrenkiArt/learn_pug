- var navLinks = [{link: "./index.html", name: "index.html"}, {link: "./page.html", name: "page.html"}, {link: "#", name: "3"}, {link: "#", name: "4", last: true}];

mixin navLink(link, name, last)
  li
    if(last)
      a(href=link).nav-link.last-link=name
    else
      a(href=link).nav-link=name

header.header
  ul.nav
    for i in navLinks
      +navLink(i.link, i.name, i.last)
